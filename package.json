{
  "name": "composer_controller",
  "version": "1.0.0",
  "description": "A controller made by composer for composers",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "release": "build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alearcy/composer_controller.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Alessandro Arcidiaco",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "lowdb": "^1.0.0",
    "osc-js": "^2.0.3",
    "socket.io": "^2.2.0",
    "webmidi": "^2.3.3",
    "ws": "^6.2.1"
  },
  "build": {
    "appId": "it.alessandroarcidiaco.${name}",
    "productName": "Composer Controller",
    "directories": {
      "buildResources": "."
    },
    "mac": {
      "category": "public.app-category.music",
      "target": "dmg"
    },
    "linux": {
      "category": "Audio"
    }
  }
}

